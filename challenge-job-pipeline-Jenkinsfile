pipeline {
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    agent {
      docker {
          image 'node:latest'
      }
    }
    stage('npm version'){
      steps{
        sh "npm -v"
      }
    }
  }
  post {
    always {
      sh 'echo "This will always run"'
    }
  }
}
